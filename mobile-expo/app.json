{
  "expo": {
    "name": "Food Stall Discovery",
    "slug": "foodstall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_with_text.png",
    "splash": {
      "image": "./assets/logo_with_text.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "foodstallapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.foodstall.mobile",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cb9557df-5f30-428d-bef7-d2d0229eccc7"
      }
    }
  }
}